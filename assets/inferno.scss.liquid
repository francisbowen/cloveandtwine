/*================ Quote slider ================*/

.quote-icon {
  display: block;
  margin: 0 auto 20px;
}

// Text styles
.quotes-slider__text {
  font-weight: $font-weight-normal;
  font-style: normal;
  padding: 0 15px;

  cite {
    font-style: normal;
  }

