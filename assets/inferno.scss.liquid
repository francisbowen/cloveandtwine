}

.image-bar__overlay {
  @include overlay;
}

.animsition.finished .image-bar__overlay {
  @include multiply;
}

.image-bar__caption {
  position: absolute;
  top: 50%;
  @include transform(translateY(-50%));
  transition: $transition-link-hover;
  width: 100%;
  text-align: center;
