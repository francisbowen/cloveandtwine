  left: 0;

  &:before {
    content: '\f053';
    display: block;
  }

  [dir="rtl"] & {
    left: auto;
    right: -25px;
  }

  .vertical-thumbs & {
    bottom: auto;
    top: 0;

    &:before {
      content: '\f077';
      left: 50%;
      @include transform(translateX(-50%));
    }
  }
}

.slick-next {
  right: 0;

  &:before {
    content: '\f054';
    display: block;
    right: 0;
  }

  [dir="rtl"] & {
    left: -25px;
    right: auto;
  }

  .vertical-thumbs & {
    bottom: 0;
    top: auto;

    &:before {
      content: '\f078';
      right: auto;
      left: 50%;
